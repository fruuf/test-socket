{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "mocha": true,
    "browser": true
  },
  "parserOptions": {
    "ecmaVersion": 8
  },
  "rules": {
    "global-require": 0,
    "import/no-unresolved": 0,
    "import/no-absolute-path": 0,
    "react/jsx-filename-extension": 0,
    "import/extensions": 0
  },
  "globals": {
    "expect": true,
    "process": true,
    "mount": true,
    "fetch": true
  }
}